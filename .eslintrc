{
  "extends": [
    "next"
  ],
  "rules": {
    "comma-dangle": [1, "always-multiline"],
    "consistent-return": 0,
    "default-case": [0],
    "import/first": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-named-as-default": 0,
    "import/prefer-default-export": 0,
    "no-console": 0,
    "no-fallthrough": 0,
    "no-multiple-empty-lines": [2, {"max": 1, "maxEOF": 1}],
    "object-curly-spacing": ["error", "always"],
    "semi": ["error", "never"]
  },
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  }
}
